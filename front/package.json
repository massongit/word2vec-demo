{
  "name": "word_embedding-demo",
  "author": "Masaya Suzuki",
  "email": "suzukimasaya428@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/massongit/word_embedding-demo"
  },
  "version": "0.0.6",
  "license": "MIT",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bootstrap": "^4.1.3",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.5.2",
    "react-intl": "^2.7.1",
    "react-redux": "^5.0.7",
    "react-scripts": "^3.3.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1"
  },
  "devDependencies": {
    "deepcopy": "^2.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-react-intl": "^2.0.5",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-react-intl": "^1.1.2",
    "eslint-plugin-react-redux": "^2.3.0",
    "jest-fetch-mock": "^1.6.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "env",
        {
          "targets": {
            "useBuiltIns": true
          }
        }
      ]
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-async-to-generator"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-redux/recommended",
      "plugin:jest/recommended",
      "react-app"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "react-redux",
      "react-intl",
      "jest"
    ],
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": [
    "> 0.2%",
    "> 0.2% in JP",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not ie <= 11",
    "not OperaMini all"
  ]
}
